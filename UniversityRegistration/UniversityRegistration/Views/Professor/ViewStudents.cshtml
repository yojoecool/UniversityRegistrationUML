@model List<UniversityRegistration.Models.StudentInfo>

@{
    ViewBag.Title = "View Students";
}

<h2>View</h2>

<table class="table">
    <tr>
        <th>
            Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.ActionLink(item.Name, "ViewStudentInfo", "Professor", new { studentId = item.Id }, null)
        </td>
    </tr>
}

</table>
